  "bugs": {
    "url": "https://github.com/saajan94/React-Shopping-List/issues"
  },
  "homepage": "https://github.com/saajan94/React-Shopping-List#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.0",
    "mongoose": "^5.5.9"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
